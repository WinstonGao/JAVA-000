<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="teacher1" class="bean.Teacher">
        <property name="name" value="张三"/>
        <property name="mobile" value="13000000000"/>
        <property name="sex" value="F"/>
    </bean>

    <bean id="teacher2" class="bean.Teacher">
                <constructor-arg index="0" value="李四"/>
        <constructor-arg index="1" value="13800000000"/>
        <constructor-arg index="2" value="M"/>
    </bean>
</beans>